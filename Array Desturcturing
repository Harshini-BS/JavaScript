//tryit out:
const details=()=>{
    
return  userdetails=[
    {
        id:1,
        name1:"harshini",
        age:19,
    },
    {
         id:2,
        name2:"harshu",
        age:20,
    },{
         id:3,
        name3:"renu",
        age:25,
    }
]
}
const ud=details();
for(let names of ud){
    let user=names.name1||names.name2||names.name3;
    console.log(user);
}

output:

harshini
